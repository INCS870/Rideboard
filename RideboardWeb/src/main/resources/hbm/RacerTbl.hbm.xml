<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rideboard.data.model.RacerModel" table="RACER_TBL">
		<id name="racerId" column="RACERID" />
		<property name="firstName" column="FIRST_NAME" />
		<property name="midName" column="MID_NAME" />
		<property name="lastName" column="LAST_NAME" />
		<property name="sin" column="SIN" />
		<property name="ranking" column="RANKING" />
		<property name="userId" column="USERID" />
		
		<property name="cre_dt" column="CRE_DT" />
		<property name="last_upd_dt" column="LAST_UPD_DT" />
		<property name="cre_user" column="CRE_USER" />
		<property name="last_upd_user" column="LAST_UPD_USER" />	
	</class>
</hibernate-mapping>