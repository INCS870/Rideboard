<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rideboard.data.model.TypeModel" table="TYPE_TBL">
		<id name="typeId" column="TYPEID" />
		<property name="typeName" column="TYPE_NAME" />
		<property name="typeCode" column="TYPE_CD" />
		<property name="catCode" column="CAT_CD" />
		
		<property name="cre_dt" column="CRE_DT" />
		<property name="last_upd_dt" column="LAST_UPD_DT" />
		<property name="cre_user" column="CRE_USER" />
		<property name="last_upd_user" column="LAST_UPD_USER" />	
	</class>
</hibernate-mapping>